Road to Nationals using explicit gender\n   700→  const { coach, loading: coachLoading } = useHeadCoach(teamName, effectiveGender);\n   701→\n   702→  // Build the team key that will be used\n   703→  const teamKey = teamName ? buildTeamKey(teamName, effectiveGender) : null;\n   704→\n   705→  return (\n   706→    <div className=\"mb-4 p-3 bg-zinc-800/30 rounded-lg border border-zinc-800\">\n   707→      <div className=\"flex items-center justify-between mb-2\">\n   708→        <div className=\"flex items-center gap-2\">\n   709→          <span className=\"text-xs text-zinc-400\">Team {teamNum}</span>\n   710→          {/* Gender indicator badge */}\n   711→          {effectiveGender && (\n   712→            <span className={`px-1.5 py-0.5 rounded text-[10px] font-medium ${\n   713→              effectiveGender === 'womens'\n   714→                ? 'bg-pink-500/20 text-pink-400 border border-pink-500/30'\n   715→                : 'bg-blue-500/20 text-blue-400 border border-blue-500/30'\n   716→            }`}>\n   717→              {effectiveGender === 'womens' ? \"W\" : \"M\"}\n   718→            </span>\n   719→          )}\n   720→        </div>\n   721→        <div className=\"flex items-center gap-3\">\n   722→          {/* Logo status */}\n   723→          {logoStatus === 'has-url' && (\n   724→            <span className=\"flex items-center gap-1 text-xs text-green-400\">\n   725→              <CheckCircleIcon className=\"w-3 h-3\" /> Logo set\n   726→            </span>\n   727→          )}\n   728→          {logoStatus === 'in-library' && (\n   729→            <span className=\"flex items-center gap-1 text-xs text-blue-400\">\n   730→              <CheckCircleIcon className=\"w-3 h-3\" /> In library\n   731→            </span>\n   732→          )}\n   733→          {logoStatus === 'missing' && (\n   734→            <Link\n   735→              to=\"/media-manager\"\n   736→              className=\"flex items-center gap-1 text-xs text-amber-400 hover:text-amber-300\"\n   737→            >\n   738→              <ExclamationTriangleIcon className=\"w-3 h-3\" /> Add logo\n   739→            </Link>\n   740→          )}\n   741→          {/* Roster status */}\n   742→          {hasRoster && rosterStats && (\n   743→            <span className=\"flex items-center gap-1 text-xs text-green-400\">\n   744→              <CheckCircleIcon className=\"w-3 h-3\" /> {rosterStats.total} athletes ({rosterStats.withHeadshots} photos)\n   745→            </span>\n   746→          )}\n   747→          {teamName && !hasRoster && (\n   748→            <Link\n   749→              to=\"/media-manager\"\n   750→              className=\"flex items-center gap-1 text-xs text-amber-400 hover:text-amber-300\"\n   751→            >\n   752→              <ExclamationTriangleIcon className=\"w-3 h-3\" /> No roster\n   753→            </Link>\n   754→          )}\n   755→        </div>\n   756→      </div>\n   757→      <div className=\"flex gap-2\">\n   758→        {/* Logo preview - use explicit gender for lookup */}\n   759→        <div className=\"w-12 h-12 bg-zinc-700 rounded-lg flex items-center justify-center overflow-hidden flex-shrink-0\">\n   760→          {(teamLogo || (teamName && getTeamLogo(teamName, effectiveGender))) ? (\n   761→            <img\n   762→              src={teamLogo || getTeamLogo(teamName, effectiveGender)}\n   763→              alt={teamName || 'Team logo'}\n   764→              className=\"w-10 h-10 object-contain\"\n   765→            />\n   766→          ) : (\n   767→            <span className=\"text-zinc-500 text-lg\">?</span>\n   768→          )}\n   769→        </div>\n   770→        <div className=\"flex-1 space-y-2\">\n   771→          <input\n   772→            type=\"text\"\n   773→            value={teamName}\n   774→            onChange={(e) => onNameChange(e.target.value)}\n   775→            placeholder={`Team ${teamNum} name`}\n   776→            required={required}\n   777→            className=\"w-